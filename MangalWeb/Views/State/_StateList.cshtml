@model IList<MangalWeb.Model.Entity.tblStateMaster>
@using MangalWeb.Model.Entity
<div class="modal-content">
    <div class="modal-body" style="height:inherit; overflow:auto;">
        <div class="panel-body panel-no-padding" style="height:inherit; overflow:auto;">
            <div id="demo">
                <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered table-hover table-fixed-header m0" id="example">
                    <thead>
                        <tr>
                            <th>State Code</th>
                            <th>State Name</th>
                            <th>Country</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (tblStateMaster item in Model)
                        {
                            <tr class="gradeX" onclick='window.document.location="/State/GetStateById?ID=@item.StateID"'>
                                <td>@item.StateCode</td>
                                <td>@item.StateName</td>
                                <td>@item.tbl_CountryMaster.CountryName</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div><!-- /.modal-content -->

