@model IList<MangalWeb.Model.Entity.tblCityMaster>
@using MangalWeb.Model.Entity
<div class="modal-content">
    <div class="modal-body" style="height:inherit; overflow:auto;">
        <div class="panel-body panel-no-padding" style="height:inherit; overflow:auto;">
            <div id="demo">
                <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered table-hover table-fixed-header m0" id="example">
                    <thead>
                        <tr>
                            <th>City Name</th>
                            <th>State Name</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (tblCityMaster item in Model)
                        {
                            <tr class="gradeX" onclick='window.document.location="/City/GetCityById?ID=@item.CityID"'>
                                <td>@item.CityName</td>
                                <td>@item.tblStateMaster.StateName</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div><!-- /.modal-content -->

