@model IList<MangalWeb.Model.Transaction.ResidenceVerificationVM>
@using MangalWeb.Model.Transaction
<div class="modal-content">
    <div class="modal-body" style="height:inherit; overflow:auto;">
        <div class="panel-body panel-no-padding" style="height:inherit; overflow:auto;">
            <div id="demo1">
                <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered table-hover table-fixed-header m0" id="example">
                    <thead>
                        <tr>
                            <th style="text-align:left;">Transaction Id</th>
                            <th style="text-align:left;">Customer Id</th>
                            <th style="text-align:left;">Transaction Date</th>
                            <th style="text-align:left;">Date of Visit</th>
                            <th style="text-align:left;">Person Visited Name</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (ResidenceVerificationVM item in Model)
                        {
                            <tr class="gradeX" style="cursor:pointer;" onclick='window.document.location="/ResidenceVerification/GetResidenceVerificationById?Id=@item.Id"'>
                                <td>@item.TransactionId</td>
                                <td>@item.CustomerId</td>
                                <td>@item.AppliedDate</td>
                                <td>@item.DateofVisit</td>
                                <td>@item.PersonVisitedName</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div><!-- /.modal-content -->

