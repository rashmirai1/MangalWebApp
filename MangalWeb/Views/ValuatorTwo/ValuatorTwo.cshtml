        $("#btnEdit").click(function () {

            $("#tblValuationDetails tbody").empty();
            $("#divValuationDetails").hide();


            $('.ValuatorTwoDetailsVM_PurityId').css({ 'background-color': 'transparent' });
            $('.grossweight').css({ 'background-color': 'transparent' });
            $('.deductions').css({ 'background-color': 'transparent' });

            $('.ValuatorTwoDetailsVM_PurityId').removeAttr("disabled");
            $('.grossweight').removeAttr("disabled");
            $('.deductions').removeAttr("disabled");

            $("#headerTotalValuation").css('display', 'none');
            $("#lblTotalValuation").css('display', 'none');
            $("#lblTotalValuation").text(0);
            $("#EligibleLoanAmount").val(0);
            $("#SanctionLoanAmount").val(0);
            $("#LTVPerc").val(0);

        });

        $("#btnEvaluate").click(function () {

