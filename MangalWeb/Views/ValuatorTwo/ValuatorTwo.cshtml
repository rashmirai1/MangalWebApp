                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (result) {

                    $("#btnEdit").show();
                    $("#btnEvaluate").show();

                    $("#CustomErr").text("");

                    $("#tblValuationTwoDetails tbody tr").empty();
                    $("#divValuationTwoDetails").show();
                    $("#tblValuationDetails tbody tr").empty();
                    $("#divValuationDetails").show();

                    $.each(result, function (key, value) {
                        $("#" + key + "").val(value);
                    });

                    $("#ConsolidatedPhoto").empty();

                    if (result.ImageName != null) {
                        $("#ConsolidatedPhoto").append('<td><a href="/ValuatorTwo/Download/' + result.ID + '" target=_blank>' + result.ImageName + '</a></td>');
                    }
                    var coperation = $("#operation").val();
                    if (coperation == "Edit") {
                        $("#btnDelete").removeClass('disabled')
                    }
                    else {
                        $("#btnDelete").addClass('disabled')
                    }
                    if (result.RecordExist == false) {
                        $("#btn_edit").removeClass('disabled')
                        $("#btn_save").removeClass('disabled')
                    }
                    else {
                        $("#btn_edit").addClass('disabled')
                        $("#btn_save").addClass('disabled')
                    }
                    var TotalValuation = 0;
                    $.ajax({
                        type: 'POST',
                        url: '@Url.Action("GetPurity","ProductRate")', // we are calling json method
                        dataType: 'json',
                        data: { id: $("#ProductId").val() },
                        // here we are get value of selected country and passing same value
                        //as inputto json method GetStates.
                        success: function (data) {
                            $.each(result.ValuatorTwoDetailsList, function (key, val) {
                                let select = `<select class='ValuatorTwoDetailsVM_PurityId'><option value="">--Select Purity--</option></select>`;
                                var rowCount = $('#tblValuationTwoDetails >tbody >tr').length + 1;
                                $('#tblValuationTwoDetails tbody').append('<tr id=' + val.ID + '>' +
                              '<td width="10%" id=' + val.ValOneDetailsId + '>' + rowCount + '</td>' +
                              '<td width="20%" id="' + val.OrnamentId + '">' + val.OrnamentName + '</td>' +
                              '<td width="10%"><a href="/ValuatorTwo/DownLoadValuationImage/' + val.ID + '" target=_blank>' + val.ImageName + '</a>' +
                              '<td width="8%">' + val.Qty + '</td>' +
                              '<td> ' + select + '</td>' +
                              '<td width="8%"><input type=text class=grossweight style=width:80px; value="' + val.GrossWeight + '"/></td>' +
                              '<td width="8%"><input type=text class=deductions style=width:80px; value="' + val.Deductions + '"/></td>' +
                              '<td width="8%" class=NetWeight>' + val.NetWeight + '</td>' +
                              '<td width="8%" class=Rate>' + val.Rate + '</td>' +
                              '<td width="8%" class=Val1Total>' + val.ValOneTotal.toFixed(2) + '</td>' +
                              '<td width="8%" class=Val2Total>' + val.ValTwoTotal.toFixed(2) + '</td>' +
                              '</tr>');
                            });

                            $.each(result.ValuationDetailsList, function (key, val) {
                                var rowCount = $('#tblValuationDetails >tbody >tr').length + 1;
                                $('#tblValuationDetails tbody').append('<tr id=' + val.ID + '>' +
                              '<td width="10%">' + rowCount + '</td>' +
                              '<td width="20%" id="' + val.OrnamentId + '">' + val.OrnamentName + '</td>' +
                              '<td width="10%"><a href="/ValuatorTwo/DownLoadValuationImage/' + val.ID + '" target=_blank>' + val.ImageName + '</a>' +
                              '<td width="8%">' + val.Qty + '</td>' +
                              '<td width="20%" id="' + val.PurityId + '">' + val.PurityName + '</td>' +
                              '<td width="8%">' + val.GrossWeight + '</td>' +
                              '<td width="8%">' + val.Deductions + '</td>' +
                              '<td width="8%">' + val.NetWeight + '</td>' +
                              '<td width="8%">' + val.Rate + '</td>' +
                              '<td width="8%">' + val.ValTwoTotal.toFixed(2) + '</td>' +
                              '</tr>');
                            });

                            $('#tblValuationTwoDetails tbody>tr').each(function (index) {
                                var row = $(this).closest("tr");
                                var selectClass = row.find('.ValuatorTwoDetailsVM_PurityId');
                                let ddValue = result.ValuatorTwoDetailsList[index]["PurityId"];
                                data.forEach((d) => {
                                    selectClass.append(`<option value="${d.Value}" ${ddValue == d.Value ? 'selected' : ''}>${d.Text}</option>`)
                                })
                            });
                            var totamt = 0;
                            $('#tblValuationDetails tbody>tr').each(function () {
                                var row = $(this).closest("tr");
                                totamt = totamt + parseFloat(row.find("TD").eq(9).html());
                            });
                            $("#headerTotalValuation").css('display', 'block');
                            $("#lblTotalValuation").text(parseFloat(totamt).toFixed(2));

                            var ltv = parseFloat($("#MaxLtv").val());
                            var eligibleamt = totamt * ltv / 100;
                            $("#EligibleLoanAmount").val(parseFloat(eligibleamt).toFixed(2));
                            //$("#SanctionLoanAmount").val(parseFloat(eligibleamt).toFixed(2));
                            $("#SanctionLoanAmount").val(0);
                            var ltvperc = eligibleamt / totamt * 100;
                            $("#LTVPerc").val(parseFloat(ltvperc).toFixed(2));
                        },
                        error: function (ex) {
                            alert('Failed to retrieve data .' + ex);
                        }
                    });
                }
            })
        }


