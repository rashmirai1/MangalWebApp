            $("#tblValuationDetails tbody").empty();
            $("#btnEdit").show();

            $('#tblValuationTwoDetails input[type="text"]').prop("disabled", true);
            $('#tblValuationTwoDetails select').prop("disabled", true);
            $(':input[readonly]').css({ 'background-color': '#faf4b3' });
            $(':input[disabled]').css({ 'background-color': '#faf4b3' });

