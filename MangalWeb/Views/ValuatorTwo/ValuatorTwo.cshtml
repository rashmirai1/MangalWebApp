                    ValOneId = ID;
                }
                var OrnamentId = row.find("td").eq(1).attr('id');
                var OrnamentName = row.find("td").eq(1).html();
                var Qty = row.find("TD").eq(3).html();
                var PurityId = row.find(".ValuatorTwoDetailsVM_PurityId option:selected").val();
