@model IList<MangalWeb.Model.Masters.GstViewModel>
@using MangalWeb.Model.Masters
<div class="modal-content">
    <div class="modal-body" style="height:inherit; overflow:auto;">
        <div class="panel-body panel-no-padding" style="height:inherit; overflow:auto;">
            <div id="demo">
                <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered table-hover table-fixed-header m0" id="example">
                    <thead>
                        <tr>
                            <th>Effective From</th>
                            <th>CGST</th>
                            <th>SGST</th>
                            <th>IGST</th>
                            <th>Account Head 1</th>
                            <th>Account Head 2</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (GstViewModel item in Model)
                        {
                            <tr class="gradeX" onclick='window.document.location="/GST/GetGSTById?ID=@item.ID"'>
                                <td>@item.EffectiveFrom</td>
                                <td>@item.CGST</td>
                                <td>@item.SGST</td>
                                <td>@item.IGST</td>
                                <td>@item.CGSTAccountName</td>
                                <td>@item.SGSTAccountName</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div><!-- /.modal-content -->
