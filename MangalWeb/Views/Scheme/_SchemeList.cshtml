@model IList<MangalWeb.Model.Masters.SchemeViewModel>
@using MangalWeb.Model.Masters
<div class="modal-content">
    <div class="modal-body" style="height:inherit; overflow:auto;">
        <div class="panel-body panel-no-padding" style="height:inherit; overflow:auto;">
            <div id="demo">
                <table cellpadding="0" style="width:100%;" cellspacing="0" border="0" class="table table-striped table-bordered table-hover table-fixed-header m0" id="example">
                    <thead>
                        <tr>
                            <th>Scheme Name</th>
                            <th>Scheme Type</th>
                            <th>Product</th>
                            <th>Min. Loan Amount</th>
                            <th>Max. Loan Amount</th>
                            @*<th>Min Tenure</th>
                            <th>Max Tenure</th>*@
                            <th>Frequency</th>
                            <th>Processing Type</th>
                            <th>Processing Charges</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (SchemeViewModel item in Model)
                        {
                            <tr class="gradeX" onclick='window.document.location="/Scheme/GetSchemeById?ID=@item.SchemeId"'>
                                <td>@item.SchemeName</td>
                                <td>@item.SchemeType</td>
                                <td>@item.ProductStr</td>
                                <td>@item.MinLoanAmount</td>
                                <td>@item.MaxLoanAmount</td>
                                @*<td>@item.MinTenure</td>
                                <td>@item.MaxTenure</td>*@
                                <td>@item.Frequency</td>
                                <td>@item.ProcessingFeeType</td>
                                <td>@item.ProcessingCharges</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div><!-- /.modal-content -->

