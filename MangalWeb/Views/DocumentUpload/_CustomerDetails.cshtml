@model IList<MangalWeb.Model.Entity.TGLKYC_BasicDetails>
@using MangalWeb.Model.Entity
<div class="modal-content">
    <div class="modal-body" style="height:inherit; overflow:auto;">
        <div class="panel-body panel-no-padding" style="height:inherit; overflow:auto;">
            <div id="demo1">
                <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered table-hover table-fixed-header m0" id="CustomerDetailsTable">
                    <thead>
                        <tr>
                            <th style="text-align:left;">Customer Id</th>
                            <th style="text-align:left;">Application No</th>
                            <th style="text-align:left;">Loan Account No</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (TGLKYC_BasicDetails item in Model)
                        {
                            <tr class="gradeX" style="cursor:pointer;">
                                <td>@item.CustomerID</td>
                                <td>@item.ApplicationNo</td>
                                <td>0</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div><!-- /.modal-content -->
