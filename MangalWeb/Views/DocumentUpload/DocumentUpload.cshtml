            DocUploadViewModel.CustomerId = $('#CustomerId').val();
            DocUploadViewModel.ApplicationNo = $('#ApplicationNo').val();
            DocUploadViewModel.LoanAccountNo = $('#LoanAccountNo').val();
            DocUploadViewModel.Comments = $('#Comments').val();
